    "scores = [100,100,50,40,40,20,10]\n",
    "alice = [5,25,50,120]\n",
    "scores = sorted(list(set(scores)))\n",
    "print(scores)\n",
    "index = 0\n",
    "rank_list = []\n",
    "n = len(scores)\n",
    "for i in alice:\n",
    "    while (n > index and i >= scores[index]):\n",
    "        index += 1\n",
    "    rank_list.append(n+1-index) \n",
    "print(rank_list)"
