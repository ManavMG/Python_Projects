    "def insersort(a):\n",
    "    for i in range(1, len(a)):\n",
    "        value = a[i]\n",
    "        hole = i\n",
    "        while i>0 and a[hole-1]>value:\n",
    "            a[hole]=a[hole]-1\n",
    "            hole = hole -1\n",
    "        a[hole] = value\n",
    "a = [0,2,1,2,0]\n",
    "insersort(a)\n",
    "print(a)"
   ]
  }
