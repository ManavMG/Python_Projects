   "source": [
    "def recur_sum(n):\n",
    "    if n<=1:\n",
    "        return(n)\n",
    "    else:\n",
    "        return(n) * recur_sum(n-1)\n",
    "\n",
    "n = int(input('Enter the no. : '))\n",
    "\n",
    "print(\"The factorial of a no. is\",recur_sum(n))"
   ]
